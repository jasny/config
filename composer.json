{
    "name": "jasny/config",
    "description": "Configure your application",
    "keywords": ["config", "configuration", "ini", "json", "yaml", "db", "mysql"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/Config/issues",
        "source": "https://github.com/jasny/Config"
    },
    "require": {
        "php": ">=7.1",
        "jasny/php-functions": "^3.3",
        "psr/container": "^1.0",
        "jasny/dotkey": "^1.0"
    },
    "require-dev": {
        "ext-mysqli": "*",
        "ext-yaml": "*",
        "ext-json": "*",
        "jasny/php-code-quality": "^2.3",
      	"symfony/yaml": "^3.0",
        "nette/neon": "^2.3",
        "aws/aws-sdk-php": "^3.2",
        "phpstan/phpstan": "^0.10.2",
        "phpstan/phpstan-strict-rules": "^0.10.1"
    },
    "suggest": {
        "symfony/yaml": "To support YAML without the yaml php extension",
        "nette/neon": "Required to use the NEON notation",
        "aws/aws-sdk-php": "Required to load configuration from AWS DynamoDB"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\": "src/"
        }
    }
}
